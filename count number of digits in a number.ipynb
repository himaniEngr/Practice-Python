    "def numDig(num):\n",
    "    if num==0:\n",
    "        return 1\n",
    "    count=0\n",
    "    while num:\n",
    "        count+=1\n",
    "        num=num//10\n",
    "    return count\n",
    "n=int(input('Enter a number'))\n",
    "print('Number of digits in',n, 'is', numDig(n))"
