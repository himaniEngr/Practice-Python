    "def inverseOfArray(n,arr):\n",
    "    invArr=[0]*n\n",
    "    for i in range(n):\n",
    "        val=arr[i]\n",
    "        invArr[val]=i\n",
    "    return invArr\n",
    "\n",
    "n=int(input('enter size of array'))\n",
    "arr=list(map(int, input('enter array items').strip().split()))\n",
    "\n",
    "print('inverse of array is: ',inverseOfArray(n,arr))\n"
   ]
