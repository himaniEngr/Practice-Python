   "source": [
    "n=int(input('Enter size of array '))\n",
    "arr=list(map(int, input('Enter array items ').split()))\n",
    "m=max(arr)\n",
    "for i in range(m,0,-1):\n",
    "    for j in range(n):\n",
    "        if arr[j]>=i:\n",
    "            print('*\\t',end='')\n",
    "        else:\n",
    "            print('\\t',end='')\n",
    "    print('\\n')"
   ]
